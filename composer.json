{
  "name": "renslabs/laravel-logger-discord-channel",
  "description": "A Discord based Monolog driver for Laravel",
  "keywords": [
    "laravel",
    "logger",
    "logging",
    "discord"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "RensLabs",
      "email": "okriizaa@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0",
    "illuminate/contracts": "~7.0|~8.0|~9.0|~10.0|~11.0|~12.0",
    "illuminate/queue": "~7.0|~8.0|~9.0|~10.0|~11.0|~12.0",
    "illuminate/support": "~7.0|~8.0|~9.0|~10.0|~11.0|~12.0",
    "guzzlehttp/guzzle": "^7.0.1|^7.2",
    "monolog/monolog": "~1.12|^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "renslabs\\LoggerDiscordChannel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "renslabs\\LoggerDiscordChannel\\DiscordLoggerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "stable"
}
